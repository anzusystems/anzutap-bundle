{
  "name": "anzusystems/anzutap-bundle",
  "description": "Anzutap transformation services",
  "type": "symfony-bundle",
  "keywords": ["symfony", "anzusystems"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "AnzuSystems Community",
      "homepage": "https://github.com/orgs/anzusystems/people"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-mongodb": "*",
    "ext-redis": "*",
    "ext-zend-opcache": "*",
    "anzusystems/contracts": "^4.1",
    "anzusystems/serializer-bundle": "^5.0",
    "doctrine/collections": "^2.2",
    "mongodb/mongodb": "^2.0",
    "symfony/config": "^7.1",
    "symfony/console": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/http-client": "^7.1",
    "symfony/http-kernel": "^7.1",
    "symfony/property-info": "^7.1",
    "symfony/translation": "^7.1",
    "symfony/twig-bundle": "^7.1",
    "twig/twig": "^3.8",
    "ext-dom": "*"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^2.4",
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8.0",
    "symfony/dotenv": "^7.2",
    "symfony/runtime": "^7.2",
    "symfony/test-pack": "^1.0",
    "symfony/yaml": "^7.2",
    "symplify/easy-coding-standard": "^11.0",
    "vimeo/psalm": "^6.10"
  },
  "autoload": {
    "psr-4": {
      "AnzuSystems\\AnzutapBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnzuSystems\\AnzutapBundle\\Tests\\": "tests",
      "AnzuSystems\\AnzutapBundle\\Tests\\Data\\": "tests/data"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/runtime": true
    }
  }
}
